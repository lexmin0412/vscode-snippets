{
  "react-antd-ahooks": {
    "prefix": "=raa",
    "body": [
      "/**",
      "* @file ${2: $TM_FILENAME_BASE}",
      "* @date $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
      "*/",
      "$0",
      "",
      "import React from 'react';",
      "import { message, Modal, Table } from 'antd'",
      "import { ColumnsType } from 'antd/es/table'",
      "import { useRequest } from 'ahooks'",
      "",
      "interface Props {}",
      "const ${1} = (props: Props) => {",
      "  const { loading, data: dataSource } = useRequest<${4}[], []>(",
      "    () => getDataApi(),",
      "    { ready: true, }",
      "  )",
      "",
      "  const columns: ColumnsType<${4}> = [",
      "    {",
      "      title: '序号',",
      "      dataIndex: 'index',",
      "      key: 'index',",
      "      render: (_text, _record, index) => index + 1,",
      "    },",
      "    {",
      "      title: 'Action',",
      "      key: 'action',",
      "      render: (text, record) => (",
      "        <></>",
      "      ),",
      "    },",
      "  ];",
      "",
      "  return <Table rowKey='id' loading={loading} columns={columns} dataSource={dataSource} />",
      "};",
      "",
      "",
      "export default ${1};",
      ""
    ],
    "description": "react ahooks antd"
  }
}
